{
    "fileExtensions": [".less"],
    "attributeQuotes": true,
    "duplicateProperty": { "enabled": true },
    "emptyRule": true,
    "finalNewline": true,
    "hexValidation": true,
    "idSelector": { "enabled": true },
    "importantRule": true,
    "importPath": { "enabled": true, "exclude": ["_variables"] },
    "spaceAfterPropertyColon": { "enabled": true },
    "spaceAroundComma": { "enabled": true },
    "spaceAroundOperator": { "enabled": true },
    "spaceBeforeBrace": { "enabled": true },
    "stringQuotes": { "enabled": true },
    "trailingSemicolon": true,
    "urlQuotes": true
}